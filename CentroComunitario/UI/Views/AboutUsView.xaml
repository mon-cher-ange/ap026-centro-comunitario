<Window x:Class="UI.Views.AboutUsView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AboutUsView"
        Height="300"
        Width="400"
        WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <Style TargetType="TextBlock">
            <Setter Property="TextAlignment" Value="Center"></Setter>
            <Setter Property="Margin" Value="0,0,0,10"></Setter>
        </Style>
    </Window.Resources>
    <StackPanel HorizontalAlignment="Center"
                VerticalAlignment="Stretch">
        <TextBlock Text="Grupo 11"
                   FontSize="24"
                   FontWeight="Bold">
        </TextBlock>
        <TextBlock Text="Trabajo Práctico N.º 01"
                   FontSize="20">
        </TextBlock>
        <TextBlock TextAlignment="Center" Text="Angel Gonzalo Gutierrez"/>
        <TextBlock TextAlignment="Center" Text="Guadalupe Virginia Véliz"/>
        <TextBlock TextAlignment="Center" Text="Marcelo Alejandro Yufra"/>
        <TextBlock TextAlignment="Center" Text="Milagro Jaquelina Martinez"/>
        <TextBlock TextAlignment="Center" Text="Luciano Emanuel Apaza"/>
        <TextBlock TextAlignment="Center" Text="Marcos Augusto Tarifa"/>
        <TextBlock TextAlignment="Center" Text="Raul Sergio Funes"/>
    </StackPanel>
</Window>
